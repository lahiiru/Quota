{% set nav_path='index' %}

{% extends 'dashboard/base.html.twig' %}

{% block content %}
    <div class="row">
        <div class="col-md-12">
            <div class="box box-success box-solid">
                <div class="box-header with-border">
                    <h3 class="box-title">Quota Windows App</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <p>
                        Download and install Quota windows application in your clients' computers.
                    </p>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="box box-success box-solid">
                                <!-- /.box-header -->
                                <div class="box-body">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <p class="center-block">
                                            Setup executable for Windows 8 or later operating systems which has .NET 4.0
                                        </p>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 col-md-offset-4">
                                            <img class="" style="width:100%" src="{{ asset('bundles/images/svg/logotype192.svg') }}"/>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-top:20px;">
                                        <div class="col-md-12">
                                            <a href="{{ path('download') }}?win8=" class="btn btn-block btn-primary btn-lg">
                                                <i class="fa fa-download"></i>
                                                Download
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <!-- /.box-body -->
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="box box-success box-solid">
                                <!-- /.box-header -->
                                <div class="box-body">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <p class="text-center">
                                                Setup executable for Windows 7 or previous operating systems which has .NET 2.0
                                            </p>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 col-md-offset-4">
                                            <img class="" style="width:100%" src="{{ asset('bundles/images/svg/windows47.svg') }}"/>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-top:20px;">
                                        <div class="col-md-12">
                                                <a href="{{ path('download') }}?win7=" class="btn btn-block btn-primary btn-lg">
                                                    <i class="fa fa-download"></i>
                                                    Download
                                                </a>
                                        </div>
                                    </div>
                                </div>
                                <!-- /.box-body -->
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="box box-success box-solid">
                                <!-- /.box-header -->
                                <div class="box-body">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <p class="center-block">
                                                Android APK file for clients android OS devices. This product is not developed yet.
                                            </p>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4 col-md-offset-4" >
                                            <img class="" style="width:100%" src="{{ asset('bundles/images/svg/system1.svg') }}"/>
                                        </div>
                                    </div>
                                    <div class="row" style="margin-top:20px;">
                                        <div class="col-md-12">
                                            <button type="button" class="btn btn-block btn-primary btn-lg" disabled>
                                                <i class="fa fa-download"></i>
                                                Download
                                            </button>
                                        </div>
                                    </div>
                                </div>
                                <!-- /.box-body -->
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
    </div>
{% endblock %}